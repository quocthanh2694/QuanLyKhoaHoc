        font-size: 12pt;
        font-family: Times New Roman, Arial, Helvetica, sans-serif;
        color: #000000;
        font-weight: normal;
        line-height: 1.2;
    }

    .bordertable {
        border-color: #000000;
        border-width: 1px;
        border-style: solid;
        border-collapse: collapse;
    }

    .borderDOT {
        border-color: #000000;
        border-width: 1px;
        border-style: dotted;
        border-collapse: collapse;
    }

    P.breakhere {
        page-break-before: always;
    }

    tr td div {
        margin: 10px 0 10px 0;
    }

    table tr {
        height: 30px;
    }

    .align_left {
        text-align: center !important;
    }

    .tr_Height_0px {
