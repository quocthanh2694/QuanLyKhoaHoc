                                    <th class="align_left">Nói</th>
                                    <th class="align_left">Đọc</th>
                                    <th class="align_left">Viết</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.LyLichKhoaHoc_NgoaiNgu)
                                {

                                    <tr>
                                        <td align="center">@item.NgoaiNgu.Ngoaingu1</td>
                                        <td align="center">@item.Nghe</td>
                                        <td align="center">@item.Noi</td>
                                        <td align="center">@item.Doc</td>
                                        <td align="center">@item.Viet</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                </td>
            </tr>



        </table>
